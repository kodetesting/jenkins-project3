pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Build Project...!!'
            }
        }

        stage('test') {
            steps {
                echo 'Running TestCases within Project.'
            }
        }

        stage('deploy') {
            steps {
                echo 'Finally Deploying the Build/Project :)'
            }
        }
    }
}